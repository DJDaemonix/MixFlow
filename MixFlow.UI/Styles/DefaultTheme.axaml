<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <Styles.Resources>
    <SolidColorBrush x:Key="Window.ActiveTitlebarBackgroundBrush" Color="{DynamicResource Window.ActiveTitlebarBackgroundColor}"/>
    <SolidColorBrush x:Key="Window.InactiveTitlebarBackgroundBrush" Color="{DynamicResource Window.InactiveTitlebarBackgroundColor}"/>
    <SolidColorBrush x:Key="Window.TitlebarForegroundBrush" Color="{DynamicResource Window.TitlebarForegroundColor}"/>
  </Styles.Resources>
  
</Styles>